<main class="uk-section uk-section-default">
    <div class="uk-container">
      <h1>Teaching</h1>
      <ul class="uk-list">
        <li><span data-uk-icon="chevron-right"></span> <a href="https://webis.de/for-students/completed-theses.html#filter:mentor:martin+potthast">Student Theses</a></li>
        <li><span data-uk-icon="chevron-right"></span> <a href="https://webis.de/lecturenotes.html">Lecturenotes</a></li>
      </ul>
      <ul class="uk-list">
        <li><span data-uk-icon="chevron-down"></span>  <a href="#ws22">WS 2022/23</a></li>
        <li><span data-uk-icon="chevron-down"></span>  <a href="#previous-semesters">Previous Semesters</a></li>
      </ul>
      {% for semester in semesters %}
        <h2 id="{{ semester.id }}">{{ semester.name }}</h2>
        <ul class="uk-list uk-list-bullet">
          {% for course in semester.courses %}
            <li><a href="{{ course.url }}">{{ course.name }}</a></li>
          {% endfor %}
        </ul>
      {% endfor %}
    </div>
  </main>